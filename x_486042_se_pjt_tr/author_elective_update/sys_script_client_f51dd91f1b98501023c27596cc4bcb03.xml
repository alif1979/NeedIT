        <messages/>
        <name>co_duration</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

   //Type appropriate comment here, and begin script below
	if (g_form.getValue("change_start_time")) {
		var ga = new GlideAjax("getTimeDifference");
		ga.addParam('sysparm_name', 'timeDifference');
		ga.addParam('sysparm_start', g_form.getValue("change_start_time"));
		ga.addParam('sysparm_end', newValue);
		ga.getXML(durationCallBackFunction);
	}
		
	function durationCallBackFunction(response) {
		var answer = response.responseXML.documentElement.getAttribute('answer');
		g_form.setValue('cr_duration', answer);
	}	
   
}]]></script>
