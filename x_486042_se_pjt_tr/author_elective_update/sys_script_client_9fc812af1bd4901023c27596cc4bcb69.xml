        <name>CHG_cr_endtime_update</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }

	var ga = new GlideAjax("getTimeDifference");
	ga.addParam('sysparm_name', 'timeDifference');
	ga.addParam('sysparm_start', g_form.getValue("cr_start_time"));
	ga.addParam('sysparm_end', g_form.getValue("cr_end_time"));
	ga.getXML(callBackFunction);
	alert(ga.sysparm_start);
	alert(ga.sysparm_end);

	function callBackFunction(response) {
		var answer = response.responseXML.documentElement.getAttribute('answer');
		g_form.setValue('se_hours_consumed', answer);
	}	
   
}]]></script>
