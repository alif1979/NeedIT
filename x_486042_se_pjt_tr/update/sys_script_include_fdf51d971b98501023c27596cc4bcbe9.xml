        <script><![CDATA[var PullUserNameDetails = Class.create();
PullUserNameDetails.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	getUserDetails: function() {
		var gr = new GlideRecord("sys_user");
		gr.get(this.getParameter("sysparm_uname"));
		gr.query();
		if (gr.next()) {
			var gc = [gr.getValue("first_name"), gr.getValue("last_name")];
			return gc.toString();
		}
	},	
	
	
    type: 'PullUserNameDetails'
