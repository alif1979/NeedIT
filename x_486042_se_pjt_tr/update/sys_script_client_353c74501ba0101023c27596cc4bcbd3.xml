        <name>CO_pending_hours_update</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading) {
      return;
   }

	
	if (newValue == 0) {
		g_form.setValue("comp_off_status", "Taken");
		return;
	} else if (!g_form.getValue("se_change_hours_consumed")){
		g_form.setValue("comp_off_status", "Pending");
		return;
	} else if (newValue < g_form.getValue("comp_off_days")) {
		g_form.setValue("comp_off_status", "Partially Taken");
		return;
	}

	
   
}]]></script>
